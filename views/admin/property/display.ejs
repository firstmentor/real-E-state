<%- include('../navbar.ejs') %>
  <%- include('../sidebar.ejs') %>


    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <div class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1 class="m-0 text-dark">property</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
              <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item"><a href="#">property</a></li>
                <li class="breadcrumb-item active">property Page</li>
              </ol>
            </div><!-- /.col -->
          </div><!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content-header -->

      <!-- Main content -->
      <div class="content">
        <div class="container-fluid">

          <div class="card">
            <div class="card-header">


              <!-- <h3 class="card-title">DataTable with default features</h3> -->
              <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#modal-default">
                Add property
              </button>
              <br>
              <% if(success.length>0){ %>
                <div class="alert alert-success">
                  <%= success %>
                </div>

                <% } %>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                  <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>price</th>
                    <th>bedrooms</th>
                    <th>beds</th>
                    <th>squarefit</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Image</th>

                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  <% let i = 1 %>
                  <% p.forEach((item)=>{ %>
                    <tr>
                      <td><%= i++ %></td>
                     
                      <td>
                        <%= item.name %>
                      </td>
                      <td>
                        <%= item.price %>â‚¹
                      </td>
                      <td>
                        <%= item.beds %>
                      </td>
                      <td>
                        <%= item.bedrooms %>
                      </td>
                      <td>
                        <%= item.squarefit %>
                      </td>
                      <!-- <td><%= item.discreption %></td>
            <td><%= item.bhk %></td> -->
                      <!-- <td><%= item.address %></td> -->
                      <td>
                        <%= item.city %>
                      </td>
                      <td>
                        <%= item.state %>
                      </td>
                      <td><img src="<%= item.image.url %>" width="100" alt=""></td>

                      <td>
                        <a href="/admin/property/view/<%= item.id %>" class="btn btn-info"><i
                            class="fa-solid fa-eye"></i></a>
                        <a href="/admin/property/edit/<%= item.id %>" class="btn btn-info"><i
                            class="fa-solid fa-pen-to-square"></i></a>
                        <a href="/admin/property/delete/<%= item.id %>" class="btn btn-info"><i
                            class="fa-solid fa-trash"></i></a>
                      </td>
                    </tr>
                    <% }) %>
                </tbody>
                <tfoot>
                  <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>price</th>
                    
                    <th>bedrooms</th>
                    <th>beds</th>
                    <th>squarefit</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Image</th>

                    <th>Action</th>
                  </tr>
                </tfoot>
              </table>
            </div>
            <!-- /.card-body -->
          </div>
          <!-- /.card -->

        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->


    <div class="modal fade" id="modal-default">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title">Add job</h4>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <form action="/admin/insertproperty" method="post" enctype="multipart/form-data">
              <div class="form-group">
                <label for="">Property Name</label>
                <input type="text" name="name" class="form-control" value="" />
                <label for="">Price</label>
                <input type="text" name="price" class="form-control" />
                <label for="">Discreption</label>
                <input type="text" name="discreption" class="form-control" />
                <label for="">Bathrooms</label>
                <input type="text" name="bedrooms" class="form-control" />
                <label for="">Beds</label>
                <input type="text" name="beds" class="form-control" />
                <label for="">SquareFit</label>
                <input type="text" name="squarefit" class="form-control" />
                <label for="">Category</label>
                <select name="category" id="" class="form-control">
                  <option value="">select category</option>
                  <% cat.forEach((c)=>{ %>
                    <option value="<%= c.title %>">
                      <%= c.title %>
                    </option>

                    <% }) %>
                </select>
                <label for="">Address</label>
                <input type="text" name="address" class="form-control" />
                <label for="">City</label>
                <input type="text" name="city" class="form-control" />
                <label for="">State</label>
                <input type="text" name="state" class="form-control" />
                <label for="">Image</label>
                <input type="file" name="image" class="form-control" />
                <center>
                  <button class="btn btn-info" type="submit">Insert</button>
                </center>
              </div>
            </form>
            <!-- <p>One fine body&hellip;</p> -->
          </div>
          <!-- <div class="modal-footer justify-content-between">
              <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
              <button type="button" class="btn btn-primary">Save changes</button>
            </div> -->
        </div>
        <!-- /.modal-content -->
      </div>
      <!-- /.modal-dialog -->
    </div>
    <!-- /.modal -->

    <%- include('../footer.ejs') %>