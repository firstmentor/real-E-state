<%- include('../navbar.ejs') %>
  <%- include('../sidebar.ejs') %>


    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
      <!-- Content Header (Page header) -->
      <div class="content-header">
        <div class="container-fluid">
          <div class="row mb-2">
            <div class="col-sm-6">
              <h1 class="m-0 text-dark">My Property Bookings</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
              <ol class="breadcrumb float-sm-right">
                <li class="breadcrumb-item"><a href="#">My Property Bookings</a></li>
                <li class="breadcrumb-item active">My Property Bookings</li>
              </ol>
            </div><!-- /.col -->
          </div><!-- /.row -->
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content-header -->

      <!-- Main content -->
      <div class="content">
        <div class="container-fluid">
            <table class="table table-bordered">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Property</th>
                    <th>Booked By</th>
                    <th>Phone</th>
                    <th>Message</th>
                    <th>Booked At</th>
                  </tr>
                </thead>
                <tbody>
                  <% bookings.forEach((b, i) => { %>
                    <tr>
                      <td><%= i+1 %></td>
                      <td><%= b.property?.title || 'Deleted Property' %></td>
                      <td><%= b.user?.name %></td>
                      <td><%= b.phone %></td>
                      <td><%= b.message %></td>
                      <td><%= b.bookedAt.toLocaleDateString() %></td>
                    </tr>
                  <% }) %>
                </tbody>
              </table>

         

        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->


    
    <!-- /.modal -->

    <%- include('../footer.ejs') %>